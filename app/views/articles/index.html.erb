<h1>Article#index</h1>
<p>Find me in app/views/article/index.html.erb</p>

<div class="text-center">
  <ul class="list">
    <% @articles.each do |arti| %>
      <li>
        <%= arti.title %>
        <%= link_to "View Post", task_path(arti.id) %>
      </li>
    <% end %>
  </ul>
  <%= link_to "Add new", new_task_path %>
</div>
